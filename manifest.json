{
  "manifest_version": 3,
  "name": "Auto Clicker CSV Bot",
  "version": "1.0",
  "description": "Lit un CSV et exécute des actions automatisées ligne par ligne.",
  "permissions": ["scripting", "activeTab"],
  "host_permissions": ["<all_urls>"],
  "action": {
    "default_popup": "popup.html",
    "default_title": "Auto CSV Bot"
  }
}
